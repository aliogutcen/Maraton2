package soru3;

public class Maymun extends MemeliHayvan{

	public Maymun(String ad, int kilo, int uzunluk) {
		super(ad, kilo, uzunluk);
		setTehlikeliMi(true);
	}

	@Override
	public void yuru() {
			System.out.println(getAd()+" isimli maymun yürüyor..");
		
	}

	@Override
	public void sesCikar() {
		System.out.println(getAd()+" isimli maymun ses çıkarıyor..");
		
	}

}
